{
  "name": "unite-hub",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:components": "vitest run tests/components",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test && npm run test:e2e",
    "test:api": "node test-api-flows.mjs",
    "check:db": "node create-tables.mjs",
    "agent:email": "node scripts/run-email-agent.mjs",
    "dev": "next dev -p 3008",
    "build": "next build",
    "vercel-build": "next build",
    "start": "next start",
    "convex": "convex dev",
    "email-agent": "node scripts/run-email-agent.mjs",
    "content-agent": "node scripts/run-content-agent.mjs",
    "orchestrator": "node scripts/run-orchestrator.mjs",
    "workflow": "node scripts/run-orchestrator.mjs full",
    "audit-system": "node scripts/run-orchestrator.mjs audit",
    "analyze-contacts": "node scripts/analyze-contacts.mjs",
    "generate-content": "node scripts/generate-content.mjs",
    "process-campaigns": "node scripts/process-campaigns.mjs",
    "test:caching": "node scripts/test-prompt-caching.mjs",
    "docker:start": "docker-compose up -d",
    "docker:start:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up -d",
    "docker:stop": "docker-compose down",
    "docker:stop:clean": "docker-compose down -v",
    "docker:build": "docker-compose build",
    "docker:rebuild": "docker-compose down && docker-compose build --no-cache && docker-compose up -d",
    "docker:logs": "docker-compose logs -f",
    "docker:logs:app": "docker-compose logs -f app",
    "docker:shell": "docker-compose exec app sh",
    "docker:health": "docker-compose ps && curl http://localhost:3008/api/health"
  },
