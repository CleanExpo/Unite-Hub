# =====================================================
# Unite-Hub Multi-Agent System - Environment Variables
# =====================================================

# =====================================================
# 1. SUPABASE (REQUIRED)
# =====================================================
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# =====================================================
# 2. ANTHROPIC AI (REQUIRED)
# =====================================================
ANTHROPIC_API_KEY=sk-ant-your-key-here

# =====================================================
# 3. OPENROUTER (FALLBACK - OPTIONAL)
# =====================================================
OPENROUTER_API_KEY_2=your-openrouter-key

# =====================================================
# 4. OPENAI (FOR WHISPER TRANSCRIPTION)
# =====================================================
OPENAI_API_KEY=sk-your-openai-key

# =====================================================
# 5. GOOGLE OAUTH (FOR GMAIL INTEGRATION)
# =====================================================
GOOGLE_CLIENT_ID=your-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-client-secret

# =====================================================
# 6. RABBITMQ (OPTIONAL OVERRIDES)
# =====================================================
RABBITMQ_USER=unite_hub
RABBITMQ_PASSWORD=unite_hub_pass

# =====================================================
# 7. REDIS (OPTIONAL - FOR CACHING)
# =====================================================
REDIS_URL=redis://redis:6379

# =====================================================
# 8. AGENT CONFIGURATION (OPTIONAL)
# =====================================================

# Orchestrator
ORCHESTRATOR_CONCURRENCY=5

# Email Agent
EMAIL_AGENT_CONCURRENCY=3

# Content Agent (Extended Thinking)
ENABLE_EXTENDED_THINKING=true
THINKING_BUDGET_TOKENS=7500

# Campaign Agent
CAMPAIGN_AGENT_CONCURRENCY=3

# Continuous Intelligence Agent
MONITOR_INTERVAL_SECONDS=300
LOOKBACK_MINUTES=5

# Contact Intelligence Agent
CONTACT_AGENT_CONCURRENCY=3

# Media Transcription Agent
MEDIA_AGENT_CONCURRENCY=5

# Email Integration Agent
EMAIL_INTEGRATION_CONCURRENCY=3

# Analytics Agent
ANALYTICS_AGENT_CONCURRENCY=2

# =====================================================
# 9. WORKSPACE IDS (FOR TESTING)
# =====================================================
